{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "host": {
        "type": "string"
    },
    "clusterName": {
        "type": "string",
        "pattern": "^[a-zA-Z0-9](?:[a-zA-Z0-9.-]{0,61}[a-zA-Z0-9])?$"
    },
    "cloudAccountId": {
        "type": [
            "string",
            "integer"
        ]
    }
},
"required": [
    "host",
    "clusterName",
    "cloudAccountId"
]
  }